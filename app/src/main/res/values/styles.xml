<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="getstartedlogo">
        <item name="android:layout_width">390dp</item>
        <item name="android:layout_height">390dp</item>
        <item name="android:layout_marginTop">70dp</item>
    </style>

    <style name="getstartedtext">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#EE6723</item>
        <item name="android:textSize">34sp</item>
    </style>

    <style name="getstartedbutton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">65dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:backgroundTint">#EE6723</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="getstartedfacebookbutton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">65dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">100dp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:fontFamily">@font/poppins_semibold</item>
        <item name="android:drawableLeft">@drawable/ic_facebook_logo</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:backgroundTint">#1877f2</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="getstartedgooglebutton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">65dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
